exists(base_link,target_frame) -> target.found
0.2 > zdiff(base_link,map) -> landed
1.4 < zdiff(base_link,map) -> flying
1 / xydist(base_link,target_frame) ~> target.distance
1 / xydist(base_link,exp1) ~> exp1.distance
1 / xydist(base_link,exp2) ~> exp2.distance
1 / xydist(base_link,exp3) ~> exp3.distance
1 / xydist(base_link,exp4) ~> exp4.distance
0.3 > xydist(base_link,target_frame) -> target.reached
0.3 > xydist(base_link,exp1) -> exp1.reached
0.3 > xydist(base_link,exp2) -> exp2.reached
0.3 > xydist(base_link,exp3) -> exp3.reached
0.3 > xydist(base_link,exp4) -> exp4.reached
